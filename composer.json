{
    "name": "yii-extension/simple-view-bulma",
    "type": "library",
    "description": "Simple view bulma for web application yii3.",
    "keywords": [
        "yii3",
        "simple",
        "view",
        "bulma",
        "web application"
    ],
    "license": "BSD-3-Clause",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "yii-extension/flash-message-bulma": "^1.0.0",
        "yii-extension/fontawesome": "^1.0.0",
        "yiisoft/assets": "^1.0@dev",
        "yiisoft/router": "^3.0@dev",
        "yiisoft/router-fastroute": "^3.0@dev",
        "yiisoft/translator-extractor": "3.0.x-dev",
        "yiisoft/translator-formatter-intl": "^3.0@dev",
        "yiisoft/translator-message-php": "3.0.x-dev",
        "yiisoft/yii-bulma": "^1.0@dev",
        "yiisoft/yii-event": "@dev",
        "yiisoft/yii-view": "^3.0@dev"
    },
    "require-dev": {
        "codeception/c3": "^2.6.0",
        "codeception/codeception": "^4.1.20",
        "codeception/module-asserts": "^1.3.1",
        "codeception/module-phpbrowser": "^1.0.2",
        "nyholm/psr7": "^1.3.0",
        "psr/log": "^1.1",
        "roave/infection-static-analysis-plugin": "^1.7",
        "vimeo/psalm": "^4.7",
        "yiisoft/config": "^1.0@dev",
        "yiisoft/error-handler": "^3.0@dev",
        "yiisoft/yii-web": "^3.0@dev"
    },
    "autoload": {
        "psr-4": {
            "Simple\\View\\Bulma\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Simple\\View\\Bulma\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        },
        "config-plugin": {
            "params": "config/params.php",
            "web": [
                "config/web/*.php"
            ],
            "routes": "config/routes.php"
        }
    },
    "config": {
        "sort-packages": true
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
